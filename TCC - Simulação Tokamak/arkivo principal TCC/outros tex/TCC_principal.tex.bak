hold off
xlabel('Time ( ms )')
legend('I_{Plasma} ( 100 kA )','I_{OH} ( kA )','V ( kV )','R_{plasma} ( m\Omega )')%,'V_{Loop}')
axis([t1*1000-10 t6*1000+10 -4 4])

figure(2)
clf
plot(Ioh/1e3,V/1e3,'b','linewidth',2)
hold on
plot([-6 6],[0 0],'k')
plot([0 0],[-4 4],'k')
hold off
xlabel('I_{OH} ( kA )')
ylabel('V ( kV )')
axis([-4 4 -3 3])
  
